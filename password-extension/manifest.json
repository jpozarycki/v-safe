{
  "manifest_version": 3,
  "name": "NeoVault Password Viewer",
  "version": "1.0",
  "description": "Lists passwords by domain from your API.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
